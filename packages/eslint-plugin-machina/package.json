{
    "name": "eslint-plugin-machina",
    "version": "0.1.0",
    "description": "ESLint plugin for static analysis of machina FSM configs.",
    "author": "Jim Cowart (http://ifandelse.com)",
    "license": "MIT",
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.mts",
    "exports": {
        ".": {
            "types": "./dist/index.d.mts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "sideEffects": false,
    "scripts": {
        "dev": "tsdown --watch",
        "build": "tsdown",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch",
        "lint": "ESLINT_USE_FLAT_CONFIG=false eslint --config ../../.eslintrc.js 'src/**/*.{ts,tsx}'"
    },
    "peerDependencies": {
        "eslint": ">=9.0.0"
    },
    "dependencies": {
        "machina-inspect": "workspace:*"
    },
    "devDependencies": {
        "@types/eslint": "^9.6.1",
        "@types/estree": "^1.0.0",
        "eslint": "^9.39.2",
        "jest": "^30.2.0",
        "ts-jest": "^29.4.6",
        "tsdown": "^0.20.3",
        "typescript": "^5.9.3"
    }
}
